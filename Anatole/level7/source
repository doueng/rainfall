
char    *c;

void    m(void)
{
	printf("%s - %d\n", c, time(0));
}

int     main(int argc, char **argv)
{
	char	**a, **c;
	char    *b, *d;

	a = (char **)malloc(sizeof(char *) * 2);
	c = (char **)malloc(sizeof(char *) * 2);
	b = (char *)malloc(sizeof(char) * 8);
	d = (char *)malloc(sizeof(char) * 8);

	a[0] = (char *)1;
	a[1] = b;

	c[0] = (char *)2;
	c[1] = d;

	strcpy(a[1], argv[1]);
	strcpy(c[2], argv[2]);
	fd = fopen("/home/user/level8/.pass", "r");
	fgets(c, 0x44, fd);
	puts("~~");
}

